{
  "name": "e-commerce-express",
  "version": "0.0.1",
  "description": "Simples APIRest for training express, typescript and prisma",
  "main": "dist/index.js",
  "scripts": {
    "dev": "tsx watch src/server.ts",
    "start": "node build/server.js",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "build": "tsup src",
    "test": "vitest run --dir src/services",
    "test:watch": "vitest --dir src/services",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TommiL90/api-papajhons.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/TommiL90/api-papajhons/issues"
  },
  "homepage": "https://github.com/TommiL90/api-papajhons#readme",
  "devDependencies": {
    "@rocketseat/eslint-config": "^2.1.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^20.2.3",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "1.2.2",
    "@vitest/ui": "1.2.2",
    "eslint": "^8.56.0",
    "prisma": "^5.9.0",
    "supertest": "6.3.4",
    "tsup": "^8.0.1",
    "tsx": "^4.7.0",
    "typescript": "^5.0.4",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.2.2"
  },
  "dependencies": {
    "@prisma/client": "^5.9.0",
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "dayjs": "1.11.10",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-async-errors": "^3.1.1",
    "jsonwebtoken": "^9.0.0",
    "reflect-metadata": "^0.1.13",
    "zod": "^3.21.4"
  }
}
